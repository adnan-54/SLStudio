<UserControl x:Class="SLStudio.RpkEditor.Modules.RpkEditor.Views.RpkEditorView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:SLStudio.RpkEditor.Modules.RpkEditor.Views"
             mc:Ignorable="d" d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="{StaticResource SmallFullMargin}">
        <TabControl TabStripPlacement="Bottom" Padding="0">
            <TabItem Header="Code">
                <TextBox BorderThickness="0" />
            </TabItem>
            <TabItem Header="Designer">
                <ListBox>
                </ListBox>
            </TabItem>
        </TabControl>
    </Grid>
</UserControl>