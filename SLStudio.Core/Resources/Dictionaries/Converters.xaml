<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:SLStudio.Core.Converters"
                    xmlns:xcad="http://schemas.xceed.com/wpf/xaml/avalondock">

    <converters:IsNullConverter x:Key="IsNullConverter" />
    <converters:IsNullConverter x:Key="IsNotNullConverter" Negate="True" />
    <converters:NullableValueConverter x:Key="NullableValueConverter" />
    <converters:DateTimeConverter x:Key="DateTimeConverter" />
    <converters:HumanizeDateTimeConverter x:Key="HumanizeDateTimeConverter" />

    <xcad:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
</ResourceDictionary>