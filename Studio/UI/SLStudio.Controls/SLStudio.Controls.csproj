<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net5.0-windows</TargetFramework>
		<UseWPF>true</UseWPF>
		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\..\..\SolutionItems\SharedAssemblyInfo.cs" Link="SharedAssemblyInfo.cs" />
		<Compile Include="..\..\..\SolutionItems\SharedWpfInfo.cs" Link="SharedWpfInfo.cs" />
		<Compile Include="..\..\..\SolutionItems\StudioConstants.cs" Link="StudioConstants.cs" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="AvalonEdit" Version="6.1.0-preview1" />
	  <PackageReference Include="DevExpressMvvm" Version="20.2.3" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Update="StudioTextEditor_Control\Resources\TextEditorResources.Designer.cs">
	    <DependentUpon>TextEditorResources.resx</DependentUpon>
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	  </Compile>
	  <Compile Update="StudioTextEditor_Control\StudioTextEditorLeftMargin.xaml.cs">
	    <SubType>Code</SubType>
	  </Compile>
	  <Compile Update="StudioTextEditor_Control\Views\FindAndReplaceView.xaml.cs">
	    <SubType>Code</SubType>
	  </Compile>
	  <Compile Update="StudioTextEditor_Control\Views\GoToLineView.xaml.cs">
	    <SubType>Code</SubType>
	  </Compile>
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Update="StudioTextEditor_Control\Resources\TextEditorResources.resx">
	    <LastGenOutput>TextEditorResources.Designer.cs</LastGenOutput>
	    <Generator>PublicResXFileCodeGenerator</Generator>
	  </EmbeddedResource>
	</ItemGroup>

	<ItemGroup>
	  <Page Update="StudioTextEditor_Control\StudioTextEditorLeftMargin.xaml">
	    <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
	    <SubType>Designer</SubType>
	  </Page>
	  <Page Update="StudioTextEditor_Control\Views\FindAndReplaceView.xaml">
	    <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
	    <SubType>Designer</SubType>
	  </Page>
	  <Page Update="StudioTextEditor_Control\Views\GoToLineView.xaml">
	    <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
	    <SubType>Designer</SubType>
	  </Page>
	</ItemGroup>
</Project>